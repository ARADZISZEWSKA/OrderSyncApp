<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ProjektMaui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ProjektMaui"
    xmlns:views="clr-namespace:ProjektMaui.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="ProjektMaui">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent Route="LoginPage" ContentTemplate="{DataTemplate views:LoginPage}" />
    <ShellContent Route="RegisterPage" ContentTemplate="{DataTemplate views:RegisterPage}" />

</Shell>
